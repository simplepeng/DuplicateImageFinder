CREATE TABLE Project (
    id INTEGER NOT NULL PRIMARY KEY  AUTOINCREMENT,
    name TEXT NOT NULL,
    path TEXT NOT NULL,
    time INTEGER  NOT NULL
);

selectAll:
SELECT *
FROM Project
ORDER BY time DESC
;

insert:
INSERT INTO Project(name,path,time)
VALUES (?,?,?);

delete:
DELETE FROM Project
WHERE id = ?;

get:
SELECT *
FROM Project
WHERE id = ?;
